<div class="jumbotron">
  <div class="container">
    <div class='row text-center'>
    <h1>All Robots</h1>

    <% @robots.each do |robot| %>
    <img src="https://robohash.org/<%= robot.avatar %>" alt='avatar' class="center-block">
      <h3><%= robot.id %>: <a href="/robots/<%= robot.id %>"><%= robot.name %></a></h3>
    <% end %>
    </div>
  </div>
</div>
